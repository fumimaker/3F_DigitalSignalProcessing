I = imread('dachshund.jpg');
whos I;

[col, row, dep] = size(I);
row=uint32(row/32);
col=uint32(col/32);
dep=uint32(dep);
array=zeros(col, row, dep);

for d=1:dep
    for i=1:col
        for j=1:row
            upup=I(i*2, j*2, d);
            updown=I(i*2-1, j*2, d);
            downup=I(i*2, j*2-1, d);
            downdown=I(i*2-1, j*2-1, d);
            avg=
            array(i, j, d) =
        end
    end
end

image(array);